function fibonacci(n){
    var output = [];
    
    if( n === 1){
        output = [0]; 
    }
    if(n===2){
        output= [0, 1];
    }
    else{output= [0, 1];
        for (i = 2; i < n; i ++){
          output.push(output[output.lenth - 1] + output[output.lenth - 2]);
        }
    }
    return output;
}


document.write(fibonacci(9));
